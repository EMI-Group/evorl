    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                // "CUDA_VISIBLE_DEVICES": "4,5,6",
                // "JAX_DISABLE_JIT": "true"
            },
        },
        {
            "name": "Train",
            "type": "debugpy",
            "request": "launch",
            "module": "evorl.train",
            "args": [
                "agent=ppo",
                "env=brax/ant",
                // "agent=exp/ppo/acrobat",
                // "env=gymnax/Acrobot-v1",
                "debug=true",
                "checkpoint.enable=false"
                // "hydra.verbose=true",
            ],
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                // "CUDA_VISIBLE_DEVICES": "4,5,6",
                // "JAX_DISABLE_JIT": "true"
            },
            "justMyCode": false
        },
        {
            "name": "Train multirun",
            "type": "debugpy",
            "request": "launch",
            "module": "evorl.train",
            "args": [
                "-m",
                "agent=ppo",
                "env=brax/ant",
                "seed=0,1",
                "debug=true",
                "checkpoint.enable=false"
            ],
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                // "CUDA_VISIBLE_DEVICES": "4,5,6",
                // "JAX_DISABLE_JIT": "true"
            },
            "justMyCode": false
        },
        {
            "name": "PBT Train",
            "type": "debugpy",
            "request": "launch",
            "module": "evorl.train",
            "args": [
                "agent=pbt",
                "env=brax/ant",
                "debug=true",
                // "checkpoint.enable=false"
                // "hydra.verbose=true",
            ],
            "env": {
                // "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                // "CUDA_VISIBLE_DEVICES": "4,5,6",
                // "JAX_DISABLE_JIT": "true"
            },
            "justMyCode": false
        },
        {
            "name": "PBT Train nGPU",
            "type": "debugpy",
            "request": "launch",
            "module": "evorl.train",
            "args": [
                "agent=pbt-ngpu",
                "env=brax/ant",
                "debug=true",
                "checkpoint.enable=false"
                // "hydra.verbose=true",
            ],
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "CUDA_VISIBLE_DEVICES": "0,1"
                // "JAX_DISABLE_JIT": "true"
            },
            "justMyCode": false
        },
        {
            "name": "EC-SO Train",
            "type": "debugpy",
            "request": "launch",
            "module": "evorl.train",
            "args": [
                "agent=ec/cmaes",
                "env=brax/hopper",
                "debug=true",
                "checkpoint.enable=false"
            ],
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                // "CUDA_VISIBLE_DEVICES": "4,5,6",
                // "JAX_DISABLE_JIT": "true"
            },
            "justMyCode": false
        },
        {
            "name": "EC-MO Train",
            "type": "debugpy",
            "request": "launch",
